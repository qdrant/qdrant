[codespell]

exclude-file =
  Cargo.lock,
  tests/tls/cert/key.pem,

# openapi.json is typically a released version and could carry old typos
skip =
  openapi.json,
  static,
  target,

# ABD - Geohash example
# generall - The boss
# hel - Query token
# mmapped - Memory mapped
ignore-words-list =
  ABD,
  crate,
  generall,
  hel,
  mmapped,
